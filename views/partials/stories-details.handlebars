{{!-- story cards generated on view-stories with story info from database --}}
<article class="message myMessage">
    <a id="stories-story" href="/story/{{id}}">
  <div class="message-header">
    <p id="story-username">{{user.user_name}}</p>
    </div>
  <div class="message-body" id="myMessageBody">
      <h2><strong>{{title}}</strong></h2>
      <p class="summary-txt">
   {{summary}}
    </p>
  </div>
  </a>
</article>
